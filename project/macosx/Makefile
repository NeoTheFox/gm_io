ifndef config
  config=server
endif
export config

PROJECTS := LuaOO io

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

LuaOO: 
	@echo "==== Building LuaOO ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f LuaOO.make

io: LuaOO
	@echo "==== Building io ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f io.make

clean:
	@${MAKE} --no-print-directory -C . -f LuaOO.make clean
	@${MAKE} --no-print-directory -C . -f io.make clean

help:
